{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    // "module": "NodeNext",
    "strictNullChecks": true,
    "esModuleInterop": true,
    // "resolveJsonModule": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,

    "lib": ["ESNext"],
    "module": "node16",
    "target": "ESNext",
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  },
  "include": ["."]
}
